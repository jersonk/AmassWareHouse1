<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <TextView
        android:id="@+id/textCdPubOrder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/blue"
        android:layout_marginLeft="10dp"
        android:text="@string/jc_no"
        android:textSize="20dp" />

    <!-- 审核 -->
    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/textApplyTime"
        android:text="@string/VA001_Label_Check"
        android:textColor="@color/black_overlay"
        android:textSize="12dp" />

    <!-- 服务类型 -->
    <TextView
        android:id="@+id/textServiceType"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/textView4"
        android:textSize="18dp" />

    <!-- 审核时间 -->
    <TextView
        android:id="@+id/textApplyTime"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:textColor="@color/black_overlay"
        android:textSize="12dp" />
	<!-- 最晚 -->
    <TextView
        android:id="@+id/textEndTime"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/textApplyTime"
        android:textColor="@color/black_overlay"
        android:textSize="12dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textEndTime"
        android:layout_alignBottom="@+id/textEndTime"
        android:layout_toLeftOf="@+id/textEndTime"
        android:text="@string/VA001_Label_AtLatest"
        android:textColor="@color/black_overlay"
        android:textSize="12dp" />

    <!-- 实际 -->
    <TextView
        android:id="@+id/textActTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textEndTime"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/textEndTime"
        android:textColor="@color/black_overlay"
        android:textSize="12dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textActTime"
        android:layout_alignBottom="@+id/textActTime"
        android:layout_toLeftOf="@+id/textActTime"
        android:text="@string/VA001_Label_Fact"
        android:textColor="@color/black_overlay"
        android:textSize="12dp" />

    <TextView
        android:id="@+id/textRemark"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textCdPubOrder"
        android:layout_toLeftOf="@+id/textView4"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:text="@string/service_remark"
        android:textColor="@color/black_overlay" />

    <!-- 未完成 -->
    <TextView
        android:id="@+id/textFlgService"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/textActTime"
        android:layout_marginRight="10dp" />

    <TextView
        android:id="@+id/textPlanNm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textActTime"
        android:layout_alignLeft="@+id/textView9"
        android:layout_toLeftOf="@+id/textFlgService" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textRemark"
        android:text="@string/depotno" />

    <TextView
        android:id="@+id/textDepotNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textRemark"
        android:layout_toRightOf="@+id/textView1"
        android:layout_toLeftOf="@+id/textView4"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="5dp" />

</RelativeLayout>